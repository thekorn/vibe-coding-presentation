{
  "name": "slidev-theme-excali-slide",
  "version": "1.3.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.3",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.5"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.3",
    "playwright-chromium": "^1.44.1"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Virgil",
        "mono": "Fira Code"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filiphric/slidev-theme-excali-slide.git"
  },
  "bugs": {
    "url": "https://github.com/filiphric/slidev-theme-excali-slide/issues"
  },
  "homepage": "https://github.com/filiphric/slidev-theme-excali-slide#readme"
}
